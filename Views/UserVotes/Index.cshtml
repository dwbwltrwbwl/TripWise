@model IEnumerable<TripWise.Models.UserVote>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.VotedAt)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdUserNavigation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdVoteOptionNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.VotedAt)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdUserNavigation.IdUser)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdVoteOptionNavigation.IdVoteOption)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.IdUserVote">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.IdUserVote">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.IdUserVote">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
