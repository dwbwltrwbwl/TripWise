@model IEnumerable<TripWise.Models.TripParticipant>

@{
    ViewData["Title"] = "Index";
}

<h1>Index</h1>

<p>
    <a asp-action="Create">Create New</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.JoinedAt)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdParticipantRoleNavigation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdTripNavigation)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.IdUserNavigation)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.JoinedAt)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdParticipantRoleNavigation.IdParticipantRole)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdTripNavigation.IdTrip)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.IdUserNavigation.IdUser)
            </td>
            <td>
                <a asp-action="Edit" asp-route-id="@item.IdTripParticipant">Edit</a> |
                <a asp-action="Details" asp-route-id="@item.IdTripParticipant">Details</a> |
                <a asp-action="Delete" asp-route-id="@item.IdTripParticipant">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
